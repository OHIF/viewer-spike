.viewportOrientationMarkers
  pointer-events: none // Necessary for click-through to cornerstone element below
  
  font-size: 15px
  color: rgb(204, 204, 204)
  line-height: 18px

  .orientationMarker
    position: absolute

  .topMid
    top: 5px
    left: 50%
    
  .leftMid
    top: 47%
    left: 5px
    